<?xml version="1.0" encoding="UTF-8"?>
<network-security-config>
    <base-config cleartextTrafficPermitted="true">
        <debug-overrides>
            <trust-anchors>
                <certificates src="system" />
            </trust-anchors>
        </debug-overrides>
    </base-config>
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">https://www.whereistobuy.com/</domain>
        <domain includeSubdomains="true">http://www.whereistobuy.com/</domain>
        <domain includeSubdomains="true">http://whereistobuy.com/</domain>
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </domain-config>
</network-security-config>